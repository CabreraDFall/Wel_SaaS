{
  "version": 3,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["index.js", "config/**", "controllers/**", "routes/**", "utils/**", ".env", "database.sql", "package.json", "package-lock.json"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
